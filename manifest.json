{
  "name": "dns-clear",
  "version": "0.0.1",
  "description": "clear chrome dns cache",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "dns-clear",
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["nativeMessaging", "tabs", "background"]
}
